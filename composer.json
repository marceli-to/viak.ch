{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^2.0",
        "chinleung/laravel-locales": "^2.0",
        "chinleung/laravel-multilingual-routes": "^4.0",
        "doctrine/dbal": "^3.5",
        "drewm/mailchimp-api": "^2.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image-laravel": "^1.0",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.6",
        "maatwebsite/excel": "^3.1",
        "marceli-to/image-cache": "^1.4",
        "marceli-to/wiretap": "^1.4.0",
        "nesbot/carbon": "^2.36",
        "nunomaduro/termwind": "^2.0",
        "opcodesio/log-viewer": "^3.8",
        "spatie/laravel-model-flags": "^1.0",
        "spatie/laravel-newsletter": "^5.3",
        "spatie/laravel-translatable": "^6.0",
        "sprain/swiss-qr-bill": "^3.2",
        "stripe/stripe-php": "^9.8",
        "symfony/http-client": "^6.2",
        "symfony/mailgun-mailer": "^6.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5|^11.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
              "amirami/localizator"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
